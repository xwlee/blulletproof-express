{
  "name": "blulletproof-express",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development nodemon --ext ts --watch src --exec 'ts-node src/server.ts'",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "lint-staged": "lint-staged",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dd-trace": "^3.1.0",
    "express": "^4.18.1",
    "helmet": "^5.1.1",
    "http-status": "^1.5.2",
    "morgan": "^1.10.0",
    "typeorm": "^0.3.9",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ]
  }
}
